record(mbbo,"tpm_TM_Scale") {
	field(DESC, "THERMAL:Temperature resolution for display")
	field(SCAN,"Passive")
	field(DTYP,"Soft Channel")
	field(ZRST,"0.01 C")
	field(ONST,"0.02 C")
	field(TWST,"0.05 C")
	field(THST,"0.1 C")
	field(FRST,"0.2 C")
	field(FVST,"0.5 C")
	field(SXST,"1.0 C")
	field(ZRVL,"100")
	field(ONVL,"50")
	field(TWVL,"20")
	field(THVL,"10")
	field(FRVL,"5")
	field(FVVL,"2")
	field(SXVL,"1")
	field(VAL,"6")
	field(PINI,"YES")
}
