record(longout,"tpm_TMDBG") {
	field(DESC, "THERMAL:TMicro Debug")
	field(PINI,"YES")
	field(DTYP,"Ascii IP Octal")
	field(OUT,"@/tyCo/2 <debug>")
	field(DOL,"0")
	field(OMSL,"closed_loop")
}
