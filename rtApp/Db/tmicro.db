record(ai,"tpm_TMTST") {
	field(DESC, "THERMAL:TMicro Test Record")
	field(SCAN,"Passive")
	field(DTYP,"Ascii IP Octal")
	field(INP,"@/tyCo/2 <1 GETTEMP><%14k%f>")
	field(ASLO,".01")
	field(PREC,"2")
	field(EGU,"C")
}
