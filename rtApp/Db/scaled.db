record(calc,"tpm_altitude") {
	field(DESC,"SERVO:Altitude Position in Degrees")
	field(SCAN,".2 second")
	field(PHAS,"2")
	field(CALC,"A*B/C")
	field(INPA,"tpm_ALENC1 NPP NMS")
	field(INPB,"0.0140099144")
	field(INPC,"3600.0")
