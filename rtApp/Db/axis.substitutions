	{ALMTRC2,"MOTOR:Alt Second Motor Current",1000,2000}
	{ALMTRV1,"MOTOR:Alt First Motor Voltage","0","0"}
	{ALMTRV2,"MOTOR:Alt Second Motor Voltage","0","0"}
	{ALERR,"SERVO:Alt Servo Error","0","0"}
	{ALSTATE,"SERVO:Altitude State","0","0"}
}

