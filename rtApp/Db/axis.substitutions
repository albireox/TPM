file axis.template {
	pattern{NAME,DESC,HIGH,HIHI}
	{ALENC1,"SERVO:First Altitude Encoder","0","0"}
	{ALCPOS,"SERVO:Altitude Position Command","0","0"}
	{ALCVOLT,"MOTOR:Altitude Voltage Command","0","0"}
	{ALENC2,"SERVO:Second Altitude Encoder","0","0"}
