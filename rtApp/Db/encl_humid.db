record(calc,"tpm_EnclAlarm") {
	field(DESC,"Enclosure DP Alarm")
	field(INPA,"tpm_M1FrontDpDep.VAL CPP MS")
	field(INPB,"tpm_M2FrontDpDep.VAL CPP MS")
	field(INPC,"tpm_TrussDpDep.VAL CPP MS")
	field(INPD,"tpm_WBDpDep.VAL CPP MS")
	field(INPE,"tpm_EnclDpDep.VAL CPP MS")
	field(INPF,"tpm_DpDep.VAL CPP MS")
	field(CALC,"MIN(MIN(MIN(A,B),MIN(C,D)),MIN(E,F))")
	field(HIHI, "999")
	field(HIGH, "999")
	field(LOW, "6.0")
	field(LOLO, "3.0")
	field(HHSV, "NO_ALARM")
	field(HSV, "NO_ALARM")
	field(LSV, "MINOR")
	field(LLSV, "MAJOR")
	field(PREC,"1")
	field(EGU,"C")
}
