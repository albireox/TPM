record(calc, "tpm_AzWindErr") {
	field(DESC, "PLC:Az Windscreen Servo Error")
	field(INPA, "tpm_ABI2_03 CPP MS")
	field(CALC, "(A>32767)?((A-65536)/3276.8):(A/3276.8)")
	field(EGU, "v")
	field(HIHI, "10.0")
	field(LOLO, "-10.0")
	field(HIGH, "10.0")
	field(LOW, "-10.0")
	field(PREC, "3")
	field(HHSV, "MAJOR")
	field(HSV, "MINOR")
}
