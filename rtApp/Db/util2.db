record(calc,"tpm2_uptime") {
	field(DESC,"ADMIN:TPM2 up time")
	field(SCAN,"10 second")
	field(CALC,"A/B")
	field(INPA,"tpm2_vxTicks PP NMS")
	field(INPB,"216000")
	field(EGU,"Hours")
	field(PREC,"4")
}
record(longin,"tpm2_vxTicks") {
	field(DESC,"ADMIN:vxWorks clock ticks")
	field(DTYP,"vxWorks Variable")
	field(INP,"@vxTicks")
	field(EGU,"Ticks")
}
record(stringin,"tpm2_version") {
	field(DESC,"ADMIN:TPM Version ID")
	field(SCAN,"10 second")
	field(DTYP,"vxWorks Variable")
	field(INP,"@tpm_VERSION")
}
