record(waveform,"tpm_MIG1") {
	field(DESC,"MIG:M1 MIG")
	field(DTYP,"Ascii IP Octal")
	field(INP,"@/tyCo/7 <9><%6t%s>")
	field(FTVL,"CHAR")
	field(NELM,"120")
	field(SCAN,"2 second")
	field(FLNK,"tpm_MIG1Parse")
}
record(genSub, "tpm_MIG1Parse") {
	field(DESC,"MIG:M1 MIG Parse")
	field(INAM, "tpmMigInit")
	field(SNAM, "tpmMigProcess")
        field(EFLG, "ON CHANGE")
	field(INPA, "tpm_MIG1")
	field(FTA, "CHAR")
	field(NOA, "120")
	field(FTVA, "DOUBLE")
	field(FTVB, "DOUBLE")
	field(FTVC, "DOUBLE")
	field(FTVD, "DOUBLE")
	field(FTVE, "DOUBLE")
	field(FTVF, "DOUBLE")
}
